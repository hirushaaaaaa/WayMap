<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    android:backgroundTintMode="screen"
    android:baselineAligned="false">

    <!-- Username Field -->

    <!-- Password Field -->

    <!-- Login Button -->

    <!-- Back to Welcome Button -->

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="190dp"
        android:layout_height="169dp"

        android:contentDescription="@string/todooo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.092"
        app:srcCompat="@drawable/q" />

    <androidx.constraintlayout.widget.ConstraintLayout

        android:id="@+id/frameLayout2"
        android:layout_width="366dp"
        android:layout_height="388dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="231dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="112dp"
        android:background="@drawable/rounded_corner"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="34dp"
            android:layout_marginTop="19dp"
            android:layout_marginEnd="274dp"
            android:layout_marginBottom="350dp"
            android:text="@string/Username"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="35dp"
            android:layout_marginTop="115dp"
            android:layout_marginEnd="272dp"
            android:layout_marginBottom="254dp"
            android:text="@string/Password"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editTextText"
            android:layout_width="245dp"
            android:layout_height="48dp"
            android:layout_marginStart="57dp"
            android:layout_marginTop="46dp"
            android:layout_marginEnd="98dp"
            android:layout_marginBottom="297dp"
            android:autofillHints=""
            android:background="@drawable/rounded_cornertext"
            android:ems="10"
            android:hint="@string/Username"
            android:inputType="text"
            android:textColorHint="#9E9E9E"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="DuplicateSpeakableTextCheck" />

        <EditText
            android:id="@+id/editTextTextPassword"
            android:layout_width="245dp"
            android:layout_height="48dp"
            android:layout_marginStart="61dp"
            android:layout_marginTop="143dp"
            android:layout_marginEnd="95dp"
            android:layout_marginBottom="200dp"
            android:autofillHints=""
            android:background="@drawable/rounded_cornertext"
            android:ems="10"
            android:hint="@string/Password"

            android:inputType="textPassword"
            android:textColorHint="#9E9E9E"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="DuplicateSpeakableTextCheck,TextFields" />

        <Button
            android:id="@+id/loginbutton"
            android:layout_width="163dp"
            android:layout_height="48dp"
            android:layout_marginStart="125dp"
            android:layout_marginTop="198dp"
            android:layout_marginEnd="153dp"
            android:layout_marginBottom="142dp"
            android:backgroundTint="@color/white"
            android:text="@string/login"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.306"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/welcomebutton"
            android:layout_width="163dp"
            android:layout_height="48dp"
            android:layout_marginStart="104dp"
            android:layout_marginTop="264dp"
            android:layout_marginEnd="104dp"
            android:layout_marginBottom="76dp"
            android:backgroundTint="@color/material_dynamic_primary20"
            android:text="@string/back_to_welcome"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
